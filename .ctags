--langdef=swift
--langmap=swift:+.swift
--regex-swift=/[[:<:]](var|let)[[:>:]][[:space:]]+([^:=<]+).*$/\2/,variable/
--regex-swift=/[[:<:]]func[[:>:]][[:space:]]+([^\(\)<]+)\([^\(\)]*\)/\1/,function/
--regex-swift=/[[:<:]]class[[:>:]][[:space:]]+([^:\{<]+).*$/\1/,class/
--regex-swift=/[[:<:]]case[[:>:]][[:space:]]+([a-zA-Z0-9_]+)/\1/e,enumerators/
--regex-swift=/[[:<:]]enum[[:>:]][[:space:]]+([^:\{<]+).*$/\1/,enumeration/
--regex-swift=/[[:<:]]struct[[:>:]][[:space:]]+([^:\{<]+).*$/\1/,struct/
--regex-swift=/[[:<:]]protocol[[:space:]]+([^:\{<]+).*$/\1/,protocol/
--regex-swift=/[[:<:]]extension[[:>:]][[:space:]]+([^:\{<]+).*$/\1/E,extension/
--regex-swift=/[[:<:]]typealias[[:>:]][[:space:]]+([^:\{<]+).*$/\1/t,typealias/
