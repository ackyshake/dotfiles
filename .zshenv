skip_global_compinit=1

fpath=(~/.zsh/Completion $fpath)
path=(~/build/bin /usr/local/sbin ~/.bin $path)
manpath=(~/build/share/man $manpath)

# Environmental Variables
export BROWSER='open'
export CORRECT_IGNORE='_*'
export EDITOR=vim
export GREP_COLOR='1;33'
export KEYTIMEOUT=1
export LESS='-FigXR'
export PAGER='less'
export CC=clang
